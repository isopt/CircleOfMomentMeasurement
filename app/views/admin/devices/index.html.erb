<%= render 'tab' %>
<h3> 
  Device
</h3>
<table class="table table-striped">
  <tr>
    <th>
      #
    </th>
    <th>
      Human ID
    </th>

    <th>
      Sim Card ID
    </th>
    <th>
      Edit
    </th>
  </tr>
  <% @devices.each do |device| %>
  <tr>
    <td><%= device.id %></td>
    <td>
      <%= link_to admin_device_path(device) do %>
        <%= device.human_id %>
      <% end %>
    </td>

    <td>
      <%= link_to admin_device_path(device) do %>
      <%= device.sim_card_id %> 
      <% end %>
    </td>
    <td>
      <%= link_to 'Edit', edit_admin_device_path(device) %>
    </td>
  </tr>
  <% end %>
</table>

<%= link_to 'New Device', new_admin_device_path, class: 'btn btn-primary' %>
